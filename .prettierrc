{
      "semi": true,
      "singleQuote": true,
      "jsxSingleQuote": false,
      "tabWidth": 6,
      "useTabs": false,
      "trailingComma": "all",
      "printWidth": 120,
      "arrowParens": "always",
      "endOfLine": "lf",
      "bracketSpacing": true,
      "overrides": [
            {
                  "files": "*.tsx",
                  "options": {
                        "parser": "typescript"
                  }
            }
      ]
}